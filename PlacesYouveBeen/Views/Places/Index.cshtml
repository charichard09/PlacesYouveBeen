<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Places You've Been</title>
    <link 
      rel="stylesheet" 
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" 
      integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" 
      crossorigin="anonymous">
  </head>
  <body>
    <h1>List of All the Places I have visited!</h1>

    @using PlacesYouveBeen.Models;

    <ul>
    @foreach(Place place in Model)
    {
      <li><a href="/place/@place.Id">@place.CityName</a></li>
    }
    </ul>

    <a href='/new'>Add a new location</a>
    <br>
    <a href='/place'>See list of visited places</a>
  </body>
</html>